{
  "app": {
    "port": 8080,
    "prefix": "/api",
    "origin": "http://localhost:3000"
  },
  "logLevel": "info",
  "token": {
    "accessTokenExpiresIn": 30,
    "refreshTokenExpiresIn": 5,
    "rememberRefreshTokenExpiresIn": 30
  },
  "otp": {
    "otpLength": 6,
    "otpConfig": {
      "lowerCaseAlphabets": false,
      "upperCaseAlphabets": true,
      "specialChars": false
    }
  }
}
